  "author": "Felipe Farias @FelipeSouzaF",
  "bin": {
    "mongo-migrate": "./bin/run"
  },
  "bugs": "https://github.com/FelipeSouzaF/mongo-database-versioning/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "camelcase": "^6.2.0",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "indicative": "^7.4.4",
    "lodash.kebabcase": "^4.1.1",
    "moment": "^2.29.1",
    "mongodb": "^4.0.1",
    "require-uncached": "^2.0.0",
    "simple-git": "^2.37.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.56",
    "@types/validator": "^13.1.3",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/FelipeSouzaF/mongo-database-versioning",
  "keywords": [
    "oclif"
  ],
  "license": "Apache 2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mongo-migrate",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "FelipeSouzaF/mongo-database-versioning",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
